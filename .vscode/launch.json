    "version": "0.2.0",
    "configurations": [
        {
            "name": "[3 GPUs] Run RecBole Group",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_recbole_group.py",
            "args": [
                "-m",
                "GRU4Rec,LightGCN",
                "--dataset",
                "ml-100k",
                "--config_files",
                "./recbole/properties/overall.dev.yaml",
                "--nproc",
                "3",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "[1 GPUs] Run RecBole",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_recbole.py",
            "args": [
                "--model",
                "Pop",
                "--dataset",
                "test",
                "--config_files",
                "./recbole/properties/overall.dev.yaml",
                "--nproc",
                "1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "[3 GPUs] Run RecBole",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_recbole.py",
            "args": [
                "--model",
                "Pop",
                "--dataset",
                "test",
                "--config_files",
                "./recbole/properties/overall.dev.yaml",
                "--nproc",
                "3",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
